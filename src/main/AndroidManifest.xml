<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.u1660498.studio1assignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.u1660498.studio1assignment.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LineUpPage"
            android:label="@string/Lineup">
            <intent-filter>
                <action android:name="com.example.u1660498.studio1assignment.LINEUPPAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_tickets_page" />
        <activity
            android:name=".eventActivity"
            android:label="@string/event" />
        <activity android:name=".direction_Activity" />
        <activity android:name=".activity_direction" />
        <activity android:name=".car_Activity" />
        <activity android:name=".activity_Coach"></activity>
    </application>

</manifest>